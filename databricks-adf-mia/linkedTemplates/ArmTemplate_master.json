{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "databricks-adf-mia"
		},
		"raw_data_storage_formula1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'raw_data_storage_formula1'"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0611-014549-fsue64ho"
		},
		"raw_data_storage_formula1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://miaformula1dl.dfs.core.windows.net/"
		},
		"tr_process_formula1_data_properties_pipeline_parameters_p_window_end_date": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime\t"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "databricks-adf-mia_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"raw_data_storage_formula1_accountKey": {
						"value": "[parameters('raw_data_storage_formula1_accountKey')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"raw_data_storage_formula1_properties_typeProperties_url": {
						"value": "[parameters('raw_data_storage_formula1_properties_typeProperties_url')]"
					},
					"tr_process_formula1_data_properties_pipeline_parameters_p_window_end_date": {
						"value": "[parameters('tr_process_formula1_data_properties_pipeline_parameters_p_window_end_date')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}